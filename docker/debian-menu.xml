<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/ file:///usr/share/openbox/menu.xsd">
    <menu id="root-menu" label="Openbox 3">
        <item label="Chromium">
            <action name="Execute">
                <execute>
                    /usr/bin/chromium --no-sandbox --disable-gpu --disable-dev-shm-usage --window-position=0,0 --start-maximized
                </execute>
            </action>
        </item>
        <separator/>
        <item label="Terminal">
            <action name="Execute">
                <execute>
                    /usr/bin/st -t "SecureZ Terminal" -g 125x30
                </execute>
            </action>
        </item>
    </menu>
</openbox_menu>
